<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="cct.backend.entity.Ticket"
          name="ticketWithAssignee"
          systemProperties="true">
        <property name="subject"/>
        <property name="state"/>
        <property name="assignee"
                  view="_minimal"/>
        <property name="plannedDate"/>
        <property name="address"/>
        <property name="body"/>
        <property name="closedDate"/>
        <property name="category"
                  view="_minimal"/>
        <property name="latitude"/>
        <property name="longitude"/>
        <property name="declineReason"/>
        <property name="imageFile"
                  view="_local">
        </property>
        <property name="emergency"/>
        <property name="prioritised"/>
        <property name="executorRemark"/>
        <property name="moderatorRemark"/>
    </view>
</views>
